###  http://www.di.uminho.pt/prc2021/projeto#karlssonswinterpunch
:karlssonswinterpunch rdf:type owl:NamedIndividual ,
            :Cocktail ;
    :createdByBartender :seanmcclure ;
    :garnishWithIngredient :freshorangewheels ;
    :needQuantity :750mlkarlssonsgoldvodka ;
    :needQuantity :5ozpomegranatejuice ;
    :needQuantity :9ozcranberryjuice ;
    :needQuantity :9ozaperol ;
    :needQuantity :4ozsimplesyrupequalpartssugarandwater ;
