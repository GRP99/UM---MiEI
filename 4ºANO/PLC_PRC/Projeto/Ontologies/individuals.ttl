###  http://www.di.uminho.pt/prc2021/projeto#spice75
:spice75 rdf:type owl:NamedIndividual ,
            :Cocktail ;
    :hasCategory :cocktail ;
    :serveInGlassware :wineglass ;
    :garnishWithIngredient :orangespiral ;
    :needQuantity :6clchampagne ;
