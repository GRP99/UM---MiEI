###  http://www.di.uminho.pt/prc2021/projeto#speysideorchard
:speysideorchard rdf:type owl:NamedIndividual ,
                          :Cocktail ;
                 :createdByBartender :brianmacgregor ;
                 :createdInLocation :sanfrancisco ;
                 :garnishWithIngredient :appleslices ;
                 :needQuantity <http://www.di.uminho.pt/prc2021/projeto#1ozglenrothesbourbonreserve> ,
                               <http://www.di.uminho.pt/prc2021/projeto#15ozcdrouinpommeaudenormandie> ,
