###  http://www.di.uminho.pt/prc2021/projeto#figgythyme
:figgythyme rdf:type owl:NamedIndividual ,
                     :Cocktail ;
            :hasCategory :cocktail ;
            :needQuantity <http://www.di.uminho.pt/prc2021/projeto#1sprigthyme> ,
                          <http://www.di.uminho.pt/prc2021/projeto#1toptonicwater> ,
