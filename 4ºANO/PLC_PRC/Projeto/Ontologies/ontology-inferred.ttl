:sazerac rdf:type owl:NamedIndividual ,
                  :Cocktail ;
         :garnishWithIngredient :lemonpeel ;
         :hasCategory :ordinarydrink ;
         :needQuantity <http://www.di.uminho.pt/prc2021/projeto#1tspricard> ,
                       <http://www.di.uminho.pt/prc2021/projeto#1tspwater> ,
                       <http://www.di.uminho.pt/prc2021/projeto#1twistlemonpeel> ,
                       <http://www.di.uminho.pt/prc2021/projeto#12tspsuperfinesugar> ,
