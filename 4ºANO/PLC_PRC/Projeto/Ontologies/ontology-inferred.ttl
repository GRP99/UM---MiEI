###  http://www.di.uminho.pt/prc2021/projeto#pumpkinandspice
:pumpkinandspice rdf:type owl:NamedIndividual ,
                          :Cocktail ;
                 :createdByBartender :seanmcclure ;
                 :garnishWithIngredient :freshnutmeg ;
                 :needQuantity <http://www.di.uminho.pt/prc2021/projeto#1bsppumpkinpuree> ,
                               <http://www.di.uminho.pt/prc2021/projeto#1tincturefeebrothersblackwalnutbitters> ,
                               <http://www.di.uminho.pt/prc2021/projeto#15ozgingerbeer> ,
                               <http://www.di.uminho.pt/prc2021/projeto#15ozoatmealinfusedkarlssonsgoldvodka> ,
                               <http://www.di.uminho.pt/prc2021/projeto#25ozmolasses> ,
