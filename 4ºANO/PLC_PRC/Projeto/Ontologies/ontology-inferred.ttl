:lassi_asouthindiandrink rdf:type owl:NamedIndividual ,
                                  :Cocktail ;
                         :hasCategory :otherunknown ;
                         :needQuantity <http://www.di.uminho.pt/prc2021/projeto#12cupplainyoghurt> ,
                                       <http://www.di.uminho.pt/prc2021/projeto#12tspgroundroastedcuminseed> ,
                                       <http://www.di.uminho.pt/prc2021/projeto#14cupcoldwater> ,
