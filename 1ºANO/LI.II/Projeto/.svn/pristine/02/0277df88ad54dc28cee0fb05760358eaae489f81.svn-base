#ifndef ___STACK_H___
#define ___STACK_H___

/**
\brief Função que acrescenta um estado na stack
@param e Uma string contendo o estado
@param ep a stack
*/
void push(ESTADO *e, ESTADOPOS ep);

/**
\brief Função que testa se a stack esta vazia
@param e O estado
@returns o valor logico
*/
int isempty(ESTADO e);

/**
\brief Função que retira um estado da stack
@param e Uma string contendo o estado
@returns A stack
*/
ESTADOPOS pop(ESTADO *e);

#endif